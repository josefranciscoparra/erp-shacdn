{
  "name": "studio-admin",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:pre": "dotenv -e .env.pre -- next dev --turbopack",
    "build": "next build",
    "build:pre": "dotenv -e .env.pre -- next build",
    "start": "next start",
    "start:pre": "dotenv -e .env.pre -- next start",
    "worker": "tsx --tsconfig tsconfig.json src/server/worker.ts",
    "worker:pre": "dotenv -e .env.pre -- tsx --tsconfig tsconfig.json src/server/worker.ts",
    "lint": "next lint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky",
    "generate:presets": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' src/scripts/generate-theme-presets.ts",
    "db:studio": "npx prisma studio",
    "db:studio:pre": "dotenv -e .env.pre -- npx prisma studio",
    "db:push": "npx prisma db push",
    "db:push:pre": "dotenv -e .env.pre -- npx prisma db push",
    "db:migrate": "npx prisma migrate deploy",
    "db:migrate:pre": "dotenv -e .env.pre -- npx prisma migrate deploy",
    "db:seed": "npx prisma db seed",
    "db:seed:pre": "dotenv -e .env.pre -- npx prisma db seed",
    "db:generate": "npx prisma generate",
    "init:master": "tsx scripts/init-master-data.ts",
    "init:master:pre": "dotenv -e .env.pre -- tsx scripts/init-master-data.ts",
    "init:org-data": "tsx scripts/init-org-data.ts",
    "init:org-data:pre": "dotenv -e .env.pre -- tsx scripts/init-org-data.ts",
    "seed:departments": "tsx scripts/seed-departments-positions.ts",
    "seed:departments:pre": "dotenv -e .env.pre -- tsx scripts/seed-departments-positions.ts",
    "seed:org-init": "tsx scripts/seed-organization-init.ts",
    "seed:org-init:pre": "dotenv -e .env.pre -- tsx scripts/seed-organization-init.ts",
    "email:dev": "email dev --dir src/lib/email/templates --port 3001"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "eslint --fix"
    ]
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
